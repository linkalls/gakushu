{
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_NO_DOCTOR": "1",
        "EAS_NO_VCS": "1"
      },
      "withoutCredentials": false
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EXPO_NO_DOCTOR": "1",
        "EAS_NO_VCS": "1"
      },
      "withoutCredentials": false
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
